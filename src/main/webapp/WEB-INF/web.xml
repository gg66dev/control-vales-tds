<?xml version="1.0" encoding="UTF-8"?>
<web-app 
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
    version="2.5">
<display-name>Archetype Created Web Application</display-name>
  <servlet>
    <display-name>SolicitaValeServlet</display-name>
  	<servlet-name>SolicitaValeServlet</servlet-name>
  	<servlet-class>cl.tds.controlvales.servlets.SolicitaValeServlet</servlet-class>
  </servlet>
  
  <servlet>
    <display-name>AutorizarValeServlet</display-name>
  	<servlet-name>AutorizarValeServlet</servlet-name>
  	<servlet-class>cl.tds.controlvales.servlets.AutorizarValeServlet</servlet-class>
  </servlet>
  
  <servlet>
      <display-name>ConsultaValesServlet</display-name>
      <servlet-name>ConsultaValesServlet</servlet-name>
      <servlet-class>cl.tds.controlvales.servlets.ConsultaValesServlet</servlet-class>
  </servlet>
  
  <servlet>
      <display-name>LoginServlet</display-name>
      <servlet-name>LoginServlet</servlet-name>
      <servlet-class>cl.tds.controlvales.servlets.LoginServlet</servlet-class>
  </servlet>
  
  <servlet>
      <display-name>RegisterServlet</display-name>
      <servlet-name>RegisterServlet</servlet-name>
      <servlet-class>cl.tds.controlvales.servlets.RegisterServlet</servlet-class>
  </servlet>
  
  <servlet>
      <display-name>RegistrarEmpresaServlet</display-name>
      <servlet-name>RegistrarEmpresaServlet</servlet-name>
      <servlet-class>cl.tds.controlvales.servlets.RegistrarEmpresaServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>SolicitaValeServlet</servlet-name>
  	<url-pattern>/SolicitaValeServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
  	<servlet-name>AutorizarValeServlet</servlet-name>
  	<url-pattern>/AutorizarValeServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
  	<servlet-name>ConsultaValesServlet</servlet-name>
  	<url-pattern>/ConsultaValesServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
  	<servlet-name>RegisterServlet</servlet-name>
  	<url-pattern>/RegisterServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
  	<servlet-name>RegistrarEmpresaServlet</servlet-name>
  	<url-pattern>/RegistrarEmpresaServlet</url-pattern>
  </servlet-mapping>
</web-app>
