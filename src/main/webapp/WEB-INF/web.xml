  </servlet-mapping>

  <servlet-mapping>
  	<servlet-name>ConsultaValesServlet</servlet-name>
  	<url-pattern>/ConsultaValesServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
