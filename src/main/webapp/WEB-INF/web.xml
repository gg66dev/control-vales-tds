  <servlet>
  	<servlet-name>SolicitarValeServlet</servlet-name>
  	<display-name>SolicitarValeServlet</display-name>
  	<description>Clase para solicitar vale</description>
  	<servlet-class>cl.tds.controlvales.servlets.SolicitarValeServlet</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>AutorizarValeServlet</servlet-name>
  	<display-name>AutorizarValeServlet</display-name>
  	<description>Clase para autorizar vales</description>
  	<servlet-class>cl.tds.controlvales.servlets.AutorizarValeServlet</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>ConsultaValesServlet</servlet-name>
  	<display-name>ConsultaValesServlet</display-name>
  	<description>Clase para consultar vales</description>
  	<servlet-class>cl.tds.controlvales.servlets.ConsultaValesServlet</servlet-class>
  </servlet>
  
  <servlet>
